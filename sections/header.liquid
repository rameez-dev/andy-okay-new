<script>
  $(document).ready(function () {
    if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
      $(".mobile-b-desktop-h").show();
    /*setTimeout(function () {
		  $(".mobile-b-desktop-h").show();
    }, 200);*/
		setTimeout(function () {
      $(".Header__Wrapper").show();
      }, 1000);
    }
    else{
      $(".Header__Wrapper").show()
      $(".CLS-producthead-placeholder").hide();
    }
    var distance = $('.ProductForm__AddToCart').offset().top,
    $window = $(window);
    $window.scroll(function() {
        if ( ($window.scrollTop() >= (distance+46)) && (screen.width < 640)) {
            $('.GiftBox-LauncherFrame').addClass('newGiftboxloc');
        }
        else{
          $('.GiftBox-LauncherFrame').removeClass('newGiftboxloc'); 
        }
    });
  });
  $(window).bind('scroll', function() {
      if ($(window).scrollTop() > 25) {
          $('.mobile-logo-main').hide();
      }
  });
  
  $(document).ready(function() {
    $('.CurrencySelector__Select').select2({
      selectionCssClass: 'selectDropDownCss',
      minimumResultsForSearch: Infinity,
      dropdownCssClass : 'selectDropDownCss1' 
    }); 
  });
  $('.CurrencySelector__Select').change(function() {
    localStorage.setItem('currencyManuallyChanged', true);
    var selected_currency = this.value;
    localStorage.setItem('currency', selected_currency);
    var currentUrl = window.location.href;
    var url = new URL(currentUrl); 
    url.searchParams.set("currency", selected_currency); // setting your param
    var newUrl = url.href; 
    console.log(newUrl);
    console.log('yes changed');
    window.location.replace(newUrl);
  });
  
</script>

