<script>
  var cont = $('#new-currency-container');

  for (const [key, value] of Object.entries(regions)) {
    var currencyitems = ''
    value.sort((a,b) => countryNames[a].localeCompare(countryNames[b]));
    const useable_countries = value.filter((item) => currencyfiltered_object.hasOwnProperty(item));
    if (useable_countries.length > 0) {
      cont.append("<div class='new-currency-region'>" + key + "</div>")
      for (const key of useable_countries) {
        currencyitems = currencyitems.concat("<a name='new-currency-object' data-country='" + countryNames[key] +"' data-currency='" + countryCurrency[key] + "' data-flag='" + countryCurrencyFlag[key] + "'>" 
        + "<span><img src='https://cdn.shopify.com/s/files/1/0082/5619/2602/files/" + countryCurrencyFlag[key] + ".png' id='flags' ></span>"  
        + countryNames[key] + " (" + countryCurrency[key] + ")" + "</a>");
      }
      cont.append("<div class='new-currency-container-item'>" + currencyitems + "</div>")
    }
  }

  $(document).ready(function () { 
  $('a[name="new-currency-object"]').click(function myfunc() {
    localStorage.setItem('currencyManuallyChanged', true);
    var selected_currency = this.getAttribute('data-currency');
    var selected_flag = this.getAttribute('data-flag');
    var selected_country = this.getAttribute('data-country');
    localStorage.setItem('currency', selected_currency);
    localStorage.setItem('userCurrencyFlag', selected_flag);
    localStorage.setItem('country_name_show', selected_country);
    var currentUrl = window.location.href;
    var url = new URL(currentUrl); 
    url.searchParams.set("currency", selected_currency); // setting your param
    var newUrl = url.href; 
    window.location.replace(newUrl);
  });
  });
  
</script>