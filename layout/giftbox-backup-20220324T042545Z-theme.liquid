        'IT':'italy', //italy
        'ES':'spain', //spain
        'FR':'france' //france
      };
      if(localStorage.getItem('currencyManuallyChanged')!== 'true'){
        //credentials
        /*https://freegeoip.app, m.rameez@outlook.com, 12345678*/
        $.getJSON('https://api.freegeoip.app/json/?apikey=818c44e0-996b-11ec-a12f-d51aa21a8fa2',function(data){
          let userCountry = data.country_code;
          let userCurrency = countryCurrency[userCountry];
          let userCurrencyFlag = countryCurrencyFlag[userCountry];
          if(userCurrency == undefined){
            userCurrency = 'USD';
            userCurrencyFlag = 'USD';
          } 
          localStorage.setItem('currency', userCurrency);
          $('.CurrencySelector__Select').prev().children( 'img' ).attr( 'src', 'https://cdn.shopify.com/s/files/1/0082/5619/2602/files/' + userCurrencyFlag + '.png');
        });
      }
      if(localStorage.getItem('currencyManuallyChanged')== 'true'){
        $(document).ready(function () {
          var selected_currency = $('.CurrencySelector__Select').val();
          $('.CurrencySelector__Select').prev().children( 'img' ).attr( 'src', 'https://cdn.shopify.com/s/files/1/0082/5619/2602/files/' + selected_currency + '.png');
        });
      }
    </script>
