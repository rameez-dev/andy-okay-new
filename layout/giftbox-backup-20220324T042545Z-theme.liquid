        'SE':'SEK', //sweden
        'DK':'SEK', //denmark
        'NO':'SEK', //norway
        'AU':'AUD', //australia
        'USA':'USD', //usa
        'BE':'EUR', //belgium
        'NL':'EUR', //netherlands
        'DE':'EUR', //germany
        'IT':'EUR', //italy
        'ES':'EUR', //spain
        'FR':'EUR' //france
      };
      const countryCurrencyFlag = {
        'CA':'CAD', //canada
        'GB':'GBP', //uk
        'SE':'SEK', //sweden
        'DK':'denmark', //denmark
        'NO':'NO', //norway
        'AU':'AUD', //australia
        'USA':'USD', //usa
        'BE':'belgium', //belgium
        'NL':'netherlands', //netherlands
