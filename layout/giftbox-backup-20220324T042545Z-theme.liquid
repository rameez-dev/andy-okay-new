    <script>
      $(document).ready(function(){
        $("[data-xs-grid-stock-bar]").each(function(){
          let t = $(this);
          let stockStr = t.attr('data-xs-stock-json'),
              inv = t.attr('data-inv');
          console.log(inv);
          if( stockStr != "" && inv == "shopify"){
            const stockData = JSON.parse(stockStr);
            console.log(stockData);
            
            let totalStock = 0;
            for( let s in stockData ){
              totalStock += parseInt(stockData[s]);
            }
            let max_stock = parseInt(totalStock),
                left = parseInt(t.attr('data-xs-stock-left')),
                percentage = ( parseFloat(left) / parseFloat(max_stock) ) * 100.0;
            console.log(left);
            console.log(max_stock);
            console.log("dlfk");

            console.log(max_stock);
            console.log(left);

            if ( left < 0 ){
              left = 0;
              t.find("[data-xs-stock-spinner]").css("width", `0%`);
            }
            else {
              t.find("[data-xs-stock-spinner]").css("width", `${percentage}%`);
            }
            t.find("[data-xs-stock-text]").text(`${left}`);
            t.find("[data-xs-stock-text-total-custom]").text(`of ${max_stock} left`);
            
            if (max_stock >= left ){

              t.show();
            }
          }
        });
        
        $("[data-flickity-config]").flickity().flickity("resize");
      });
    </script>

