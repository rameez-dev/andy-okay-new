      "settings": {
        "number": "161341",
        "text": "<p><strong>Artworks Collected for Charity<\/strong><\/p>",
        "image": "shopify:\/\/shop_images\/impact_header_v2.jpg",
        "heading-color": "#ffffff",
        "text-color": "#ffffff",
